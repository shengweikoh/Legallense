{
    "version": 2,
    "public": true,
    "framework": "vite",
    "builds": [
      {
        "src": "dist/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "/dist/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/index.html"
      }
    ]
  }